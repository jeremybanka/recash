{
  "name": "@recash/example",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "esbuild example/client.tsx --outfile=example/web/client.js --servedir=example/web --serve=3001 --bundle --minify --sourcemap",
    "build": "esbuild example/client.tsx --outfile=example/web/client.js --bundle --minify --sourcemap",
    "predeploy": "yarn build:example",
    "deploy": "gh-pages -d example/web",
    "test": "jest --watch --no-coverage",
    "test:cov": "jest --watch",
    "test:once": "jest",
    "lint": "eslint \"{src,test}/**/*.ts{,x}\"",
    "lint:fix": "yarn lint -- --fix"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "recoil": "^0.7.2"
  },
  "lint-staged": {
    "*.ts{,x}": "yarn lint"
  },
  "eslintConfig": {
    "extends": [
      "@banka/react"
    ],
    "parserOptions": {
      "project": "./example/example.tsconfig.json"
    }
  },
  "homepage": "https://github.com/jeremybanka/recash#readme",
  "bugs": {
    "url": "https://github.com/jeremybanka/recash/issues"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.3.1"
  }
}
